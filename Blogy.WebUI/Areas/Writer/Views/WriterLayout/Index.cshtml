@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_AdminLayoutHeadComponent")
<body>
    <div class="container-scroller">
        @await Component.InvokeAsync("_WriterLayoutNavbarComponent")
        <div class="container-fluid page-body-wrapper">

            @await Component.InvokeAsync("_AdminLayoutThemeSettingComponent")
            @await Component.InvokeAsync("_WriterLayoutSidebarComponent")
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()

                </div>
                @await Component.InvokeAsync("_AdminLayoutFooterComponent")
            </div>
        </div>
    </div>
    @await Component.InvokeAsync("_AdminLayoutScriptComponent")

</body>

</html>
